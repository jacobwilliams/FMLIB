          MTLVZM(2)%MP(2) = MTLVZM(2)%MP(2) + L
          CALL ZMEQ(MTLVZM,RETURN_VALUE%MZM)
      ELSE
          CALL ZMI2M(INT(MBASE),MULVZM)
