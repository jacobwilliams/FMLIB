      INTENT (IN) :: MA
      TYPE(MULTI), SAVE :: MULVFM,M3LVFM
      CALL FM_UNDEF_INP(MA)
      NDSAVE = NDIG
      J = MAX(NGRD52,2)
      ND2 = MAX(2*NDIG+J,2)
