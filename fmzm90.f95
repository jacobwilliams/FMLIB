          CALL FMEQ(MTLVFM,MULVFM)
      ELSE
          CALL FMEQ(MTLVFM,MULVFM)
      ENDIF
      CALL ZMCMPX(MVLVFM,MULVFM,RETURN_VALUE%MZM)
