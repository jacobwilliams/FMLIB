      MXSAVE = MXEXP
      MXEXP = MXEXP2
      CALL FMI2M(0,M3LVFM)
      DO J = 1, SIZE(MA)
         JA = LBOUND(MA,DIM=1) + J - 1
         CALL FMEQU(MA(JA)%MFM,MULVFM,NDSAVE,NDIG)
         CALL FMADD_R1(M3LVFM,MULVFM)
      ENDDO
      MXEXP = MXSAVE
      CALL FMEQU(M3LVFM,RETURN_VALUE%MFM,NDIG,NDSAVE)
      NDIG = NDSAVE
